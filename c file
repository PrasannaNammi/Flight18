// Online C compiler to run C program online
#include <stdio.h>
#include<string.h>
int main() {
    //write c code here
char str[100];
gets(str);
int len= strlen(str);
int i=0;
while(i<len){
    
int k=i;
while(!(str[k]==' '|| str[k]=='\0')){
    

       k++; 
    }
    
    
    for(int l=k-1;l>i;l--){
    printf("%c",str[l]);
     }
     printf("\t");
     i=k+1;
     
     
     
     
}
return 0;
}
